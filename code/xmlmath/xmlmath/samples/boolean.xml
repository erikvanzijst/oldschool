<?xml version="1.0" encoding="UTF-8"?>
<!--
Example to illustrate logical operations in xmlmath. This expression evaluates
to true because all four arguments to <and> evaluate to true.
-->

<and xmlns="http://prutser.cx/xmlmath/schemas">
	<true/>
	
	<not>
		<equals>
			<false/>
			<true/>
		</equals>
	</not>
	
	<xor>
		<false/>
		<true/>
	</xor>
	
	<lessThan>
		<double value="1000.0"/>
		<infinity/>
	</lessThan>
</and>
